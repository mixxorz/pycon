{
  "name": "pycon",
  "version": "1.0.0",
  "description": "PyCon PH",
  "main": "gulpfile.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint:python": "flake8",
    "lint:styles": "sass-lint -vq",
    "lint:scripts": "eslint static/scripts",
    "lint": "npm run lint:python && npm run lint:styles && npm run lint:scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pythonph/pycon.git"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "bugs": {
    "url": "https://github.com/pythonph/pycon/issues"
  },
  "homepage": "https://github.com/pythonph/pycon#readme",
  "engines": {
    "node": "8.x.x"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-polyfill": "^6.26.0",
    "babelify": "^10.0.0",
    "breakpoint-sass": "^2.7.1",
    "breakpoint-slicer": "^2.0.0",
    "browser-sync": "2.26.3",
    "browserify": "^16.2.3",
    "browserify-shim": "^3.8.14",
    "envify": "^4.1.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "6.0.0",
    "gulp-csso": "^3.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "4.1.0",
    "gulp-plumber": "1.2.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.9",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "3.0.1",
    "js-cookie": "^2.2.0",
    "modularscale-sass": "^3.0.8",
    "normalize-scss": "^7.0.1",
    "npm-check-updates": "^2.14.2",
    "picturefill": "^3.0.3",
    "susy": "^3.0.5",
    "typi": "^3.2.0",
    "uglifyify": "^5.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-xo-space": "^0.20.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "comma-dangle": 0,
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery"
  },
  "browserslist": "> 0.25%, not dead"
}
